 
 
 <div class="boatHeader">
    <h2>Edit Boat # <%=@boat.id%></h2>
</div>

<%= form_for @boat, html: {multipart: true} do |f| %>
 
<div class="job-card">
<table style="">
 
  <tr>
  	<th>Boat name:</th>
    <td><%= f.text_field :name %></td> 
  </tr>
  <tr>
    <th>Boat location:</th>  
    <td> <%= f.text_field :location %></td> 
  </tr>
  <tr>
    <th># of containers  </th>
    <td><%= f.text_field :max_containers %></td> 
  </tr>
</table>
<%= f.submit "Update Boat" %>
<% end %>
 
  <div class="boatEditDelete">
  
  
</div>
 